type User {
  id: ID! @unique
  email: String
  name: String
  jobTitle: String
  telephone: String
  image: String
}

type UserRole {
  id: ID! @unique
  name: String
}

type Company {
  id: ID! @unique
  name: String!
  legalName: String
  countryId: String
  regNo: String
  address: String
  email: String
  country: String
  county: String
  city: String
  # users: User
}
